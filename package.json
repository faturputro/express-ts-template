{
  "name": "restful-express-ts-template",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": ">=18.18.2"
  },
  "scripts": {
    "dev": "export NODE_ENV=development && concurrently -r \"npm run watch-compile\" \"npm run watch-dev\"",
    "watch-compile": "chokidar 'src/**/*' -c 'npm run build'",
    "watch-dev": "nodemon --delay 100ms --watch \"dist/**/*\" -e js ./dist/index.js",
    "lint": "biome lint ./src",
    "build": "biome lint ./src && swc src -d dist",
    "start": "NODE_ENV=production node dist/index.js",
    "clean": "rm -rf dist"
  },
  "author": "Faturachman Dwi Putro",
  "license": "ISC",
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "aws-sdk": "^2.884.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.2",
    "chalk": "^4.1.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "nodemailer": "^6.9.8",
    "reflect-metadata": "^0.2.1",
    "sequelize": "^6.35.2",
    "sequelize-typescript": "^2.1.6",
    "uuid": "^9.0.1",
    "validatorjs": "^3.22.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.102",
    "@types/bcryptjs": "^2.4.6",
    "@types/bluebird": "^3.5.42",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "@types/nodemailer": "^6.4.14",
    "@types/uuid": "^9.0.7",
    "@types/validatorjs": "^3.15.5",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-biome": "^1.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  }
}
